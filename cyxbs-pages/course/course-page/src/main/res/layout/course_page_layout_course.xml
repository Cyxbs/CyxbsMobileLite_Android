      android:id="@+id/course_iv_dusk_unfold"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:rotation="90"
      android:scaleType="center"
      app:net_layout_endColumn="0"
      app:net_layout_endRow="9"
      app:net_layout_startColumn="0"
      app:net_layout_startRow="8"
      app:srcCompat="@drawable/course_page_ic_course_arrow"
      tools:ignore="ContentDescription" />

